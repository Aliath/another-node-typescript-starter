{
  "name": "another-node-typescript-starter",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "@typeofweb/schema": "^0.6.1",
    "body-parser": "^1.19.0",
    "change-case": "^4.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^1.4.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript-transform-paths": "^2.2.3"
  },
  "scripts": {
    "dev": "nodemon --quiet --watch \"src/**\" --ext \"ts,json,js\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node --files --compiler ttypescript src/index.ts\"",
    "start": "node dist",
    "test": "yarn run test:unit",
    "test:unit": "jest",
    "build": "rimraf dist && ttsc",
    "lint": "eslint ./src/**/** --quiet",
    "lint:fix": "eslint --fix --cache ./src/**/**"
  }
}
